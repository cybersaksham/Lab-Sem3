   "source": [
    "import re\n",
    "\n",
    "reg = \"^.*(?=.{6,20})(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[?@#$%^&+=]).*$\"\n",
    "match_reg = re.compile(reg)\n",
    "\n",
    "def checkPassword(passwd):\n",
    "    return re.search(match_reg, passwd)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "2b3ab069",
